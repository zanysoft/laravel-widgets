{
  "name": "zanysoft/laravel-widgets",
  "description": "Asynchronous widgets, reloadable widgets, console generator, caching - everything you can think of.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "widgets",
    "block",
    "module",
    "html",
    "ajax"
  ],
  "authors": [
    {
      "name": "Zany Soft",
      "email": "info@zanysoft.net",
      "homepage": "https://github.com/zanysoft"
    }
  ],
  "homepage": "https://github.com/zanysoft/laravel-widgets",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\Widgets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanySoft\\Widgets\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\Widgets\\ServiceProvider"
      ],
      "aliases": {
        "Widget": "ZanySoft\\Widgets\\Facade",
        "AsyncWidget": "ZanySoft\\Widgets\\AsyncFacade"
      }
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
